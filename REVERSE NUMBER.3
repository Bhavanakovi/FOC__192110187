#include <stdio.h>

// Function to reverse a number
int reverseNumber(int num) {
    int reversedNum = 0;

    while (num != 0) {
        int digit = num % 10;
        reversedNum = reversedNum * 10 + digit;
        num /= 10;
    }

    return reversedNum;
}

int main() {
    int number;

    printf("Enter an integer: ");
    scanf("%d", &number);

    int reversed = reverseNumber(number);
    printf("The reversed number is: %d\n", reversed);

    return 0;
}
