#include <stdio.h>

// Function to check if the number is negative
int isNegative(int num) {
    return num < 0;
}

int main() {
    int number;

    printf("Enter an integer: ");
    scanf("%d", &number);

    if (isNegative(number)) {
        printf("%d is a negative number.\n", number);
    } else {
        printf("%d is not a negative number.\n", number);
    }

    return 0;
}
