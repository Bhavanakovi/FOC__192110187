#include <stdio.h>

// Function to calculate the square of a number
float square(float num) {
    return num * num;
}

int main() {
    float number;

    printf("Enter a decimal number: ");
    scanf("%f", &number);

    float result = square(number);
    printf("The square of %.2f is %.2f\n", number, result);

    return 0;
}
